        android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="14" />

    <application android:icon="@drawable/neoseeker_swoosh" android:label="@string/app_name">
        <activity android:name=".NeoEntry"
                  android:label="@string/app_name"
                  android:theme="@style/Theme.Sherlock">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NeoMain"
                  android:label="@string/app_name">
        </activity>
        <activity android:name="OAuthCustomWeb">
		</activity>
        <activity android:label="@string/app_name" android:name=".NotificationsFragment">
		</activity>
        <activity android:label="@string/app_name" android:name=".PMListFragment">
		</activity>
    </application>
</manifest>